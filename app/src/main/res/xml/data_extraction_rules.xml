<?xml version="1.0" encoding="utf-8"?>
<!--
   Data extraction rules for KidsView app
   Excludes sensitive data from device-to-device transfers
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
-->
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive SharedPreferences from cloud backup -->
        <exclude domain="sharedpref" path="kidsview_prefs.xml"/>
        <exclude domain="sharedpref" path="admob_cooldown_prefs.xml"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude sensitive SharedPreferences from device transfer -->
        <exclude domain="sharedpref" path="kidsview_prefs.xml"/>
        <exclude domain="sharedpref" path="admob_cooldown_prefs.xml"/>
    </device-transfer>
</data-extraction-rules>